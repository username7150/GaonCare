<%if(success&& success.length){%>
    <div class="alert alert-dark" role="alert">
 <%=success %><button type="button" style="margin-left:38rem;" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>

<%if(error&& error.length){%>
    <div class="alert alert-danger" role="alert">
  <%=error%><button type="button"  class="btn-close" style="margin-left:38rem;" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>